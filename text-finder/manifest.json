{
  "name": "textFinder",
  "version": "0.9",
  "manifest_version": 2,
  "description": "按固定规则提取页面中的文字",
  "browser_action": {
    "default_icon": "icon.png" ,
    "default_title": "文字提取",
    "default_popup": "index.html"
  },
  "content_scripts": [{"matches": ["*://*/*"], "js": ["gettext.js"]}],
  "permissions": ["tabs"]
}
